1.
def soma(a, b):
    return a + b

2.
def primo(numero):
    if numero <= 1:
        return False
    if numero == 2:
        return True
    if numero % 2 == 0:
        return False
    for i in range(3, int(numero**0.5) + 1, 2):
        if numero % i == 0:
            return False
    return True

3.
def dobro(numero):
    return numero * 2

4.
def celsius_para_fahrenheit(celsius):
    return (celsius * 9/5) + 32

