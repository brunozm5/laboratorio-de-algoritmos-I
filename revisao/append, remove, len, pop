notas = []
notas = int(input("Digite uma nota"))
notas.append(notas)
notas = int(input("Digite uma nota"))
notas.append(notas)
notas = int(input("Digite uma nota"))
notas.append(notas)
print((notas[0] + notas[1] + notas[2]) / 3)

notas.sort()
print(notas)

for i in range(0,3):
    print("[",i,"]:")         #ver posições
    print(notas[i])              

for i in range(0,3):
    notas = int(input("Digite uma nota"))     #repetirá 3x - range
    notas.append(notas)
    totalPosicoes = len(notas)

notas.remove(5)                  #se tiver um 5 nas notas será removido
print(notas)
notas.pop()                      #remove o último dado inserido
print(notas)
