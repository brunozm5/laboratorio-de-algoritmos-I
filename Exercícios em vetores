4.
num = []
nums = int(input("Digite um número"))
num.append(nums)
nums = int(input("Digite um número"))
num.append(nums)
nums = int(input("Digite um número"))
num.append(nums)
nums = int(input("Digite um número"))
num.append(nums)
nums = int(input("Digite um número"))
num.append(nums)
if num[0] > num[1] and num[0] > num[2] and num[0] > num[3] and num[0] > num[4]:
    print("Maior número é o 1°", num[0])
if num[1] > num[0] and num[1] > num[2] and num[1] > num[3] and num[1] > num[4]:
    print("Maior número é o 2°", num[1])
if num[2] > num[1] and num[2] > num[0] and num[2] > num[3] and num[2] > num[4]:
    print("Maior número é o 3°", num[2])
if num[3] > num[0] and num[3] > num[1] and num[3] > num[2] and num[3] > num[4]:
    print("Maior número é o 4°", num[3])
if num[4] > num[0] and num[4] > num[1] and num[4] > num[2] and num[4] > num[3]:
    print("Maior número é o 5°", num[4])
if num[0] < num[1] and num[0] < num[2] and num[0] < num[3] and num[0] < num[4]:
    print("Menor número é o 1°", num[0])
if num[1] < num[0] and num[1] < num[2] and num[1] < num[3] and num[1] < num[4]:
    print("Menor número é o 2°", num[1])
if num[2] < num[1] and num[2] < num[0] and num[2] < num[3] and num[2] < num[4]:
    print("Menor número é o 3°", num[2])
if num[3] < num[0] and num[3] < num[1] and num[3] < num[2] and num[3] < num[4]:
    print("Menor número é o 4°", num[3])
if num[4] < num[0] and num[4] < num[1] and num[4] < num[2] and num[4] < num[3]:
    print("Menor número é o 5°", num[4])


