num.append(nums)
nums = int(input("Digite uma num"))
num.append(nums)
nums = int(input("Digite uma num"))
num.append(nums)
num1 = []
nums1 = int(input("Digite uma num"))
num1.append(nums1)
nums1 = int(input("Digite uma num"))
num1.append(nums1)
nums1 = int(input("Digite uma num"))
num1.append(nums1)

soma = []
somas = num[0] + num1[0]
soma.append(somas)
somas = num[1] + num1[1]
soma.append(somas)
somas = num[2] + num1[2]
soma.append(somas)
print(soma)


