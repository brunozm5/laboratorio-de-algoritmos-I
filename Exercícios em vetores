num.append(nums)
nums = int(input("Digite um número"))
num.append(nums)

if num[0] != num[1] and num[0] != num[2] and num[0] != num[3] and num[0] != num[4] and num[1] != num[2] and num[1] != num[3] and num[1] != num[4] and num[2] != num[3] and num[2] != num[4] and num[3] != num[4]:
    print("Todos os elementos são distintos.")
else:
    print("Os elementos não são distintos.")


12.
menu = int (input("O que deseja fazer?"))

while menu == 1:
    item = int(input("Digite um número par para inserir: "))
    if item % 2 != 0:
        print("Erro: apenas números pares são permitidos.")
    else:
        array.append(item)
    menu = int (input("O que deseja fazer?"))

while menu == 2:
    print("Itens:", array)
    menu = int (input("O que deseja fazer?"))

while menu == 3:
    item = int(input("Digite o número par que deseja retirar: "))
    array.remove(item)    
    menu = int (input("O que deseja fazer?"))

while menu == 4:
    array.clear()
    menu = int (input("O que deseja fazer?"))

while menu == 5:
    x = int(input("Digite o valor X: "))
    count = sum(1 for item in array if item > x)
    print(f"Há {count} itens maiores que {x} no array.")
    menu = int (input("O que deseja fazer?"))

while menu == 6:
    numero = int(input("Digite o número para verificar se está presente: "))
    if numero in array:
        print(f"O número {numero} está presente no array.")
    else:
        print(f"O número {numero} não está presente no array.")
    menu = int (input("O que deseja fazer?"))

while menu == 7:
    maior = max(array)
    menor = min(array)
    print(f"O maior item do array é {maior} e o menor é {menor}.")
    menu = int (input("O que deseja fazer?"))

if menu == 8:
    print("Adeus!")











