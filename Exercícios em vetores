num.append(nums)

presente = int(input("Digite um número."))
if num[0] == presente or num[1] == presente or num[2] == presente or num[3] == presente or num[4] == presente:
    print("O número que você digitou está presente no array.")
else:
    print("O número que você digitou não está presente no array.")


9.
par = 0
num = []
from random import randrange
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
nums = randrange(0,51)
num.append(nums)
if num[0] % 2 == 0:
    par += 1
if num[1] % 2 == 0:
    par += 1
if num[2] % 2 == 0:
    par += 1
if num[3] % 2 == 0:
    par += 1
if num[4] % 2 == 0:
    par += 1
if num[5] % 2 == 0:
    par += 1
if num[6] % 2 == 0:
    par += 1
if num[7] % 2 == 0:
    par += 1
if num[8] % 2 == 0:
    par += 1
if num[9] % 2 == 0:
    par += 1
print(num)
print("Pares:", par)
print("Ímpares:", 10 - par)


