12.
def menu():
    print("\nMenu:")
    print("1 - Inserir item")
    print("2 - Listar itens")
    print("3 - Retirar item")
    print("4 - Retirar todos os itens")
    print("5 - Contar quantos itens são maiores que um valor X")
    print("6 - Verificar se um determinado número está presente no array")
    print("7 - Encontrar o maior e o menor item do array")
    print("8 - Sair")

def inserir_item(array):
    item = int(input("Digite um número par para inserir: "))
    if item % 2 != 0:
        print("Erro: apenas números pares são permitidos.")
    else:
        array.append(item)
        print("Item inserido com sucesso.")

def listar_itens(array):
    if array:
        print("Itens no array:", array)
    else:
        print("O array está vazio.")

def retirar_item(array):
    item = int(input("Digite o número par que deseja retirar: "))
    if item in array:
        array.remove(item)
        print("Item retirado com sucesso.")
    else:
        print("Erro: o item não está presente no array.")

def retirar_todos_itens(array):
    array.clear()
    print("Todos os itens foram retirados.")

def contar_maiores_que(array):
    x = int(input("Digite o valor X: "))
    count = sum(1 for item in array if item > x)
    print(f"Há {count} itens maiores que {x} no array.")

def verificar_presenca(array):
    numero = int(input("Digite o número para verificar se está presente no array: "))
    if numero in array:
        print(f"O número {numero} está presente no array.")
    else:
        print(f"O número {numero} não está presente no array.")

def encontrar_maior_menor(array):
    if array:
        maior = max(array)
        menor = min(array)
        print(f"O maior item do array é {maior} e o menor é {menor}.")
    else:
        print("O array está vazio.")

def main():
    array = []
    while True:
        menu()
        opcao = int(input("Escolha uma opção: "))
        if opcao == 1:
            inserir_item(array)
        elif opcao == 2:
            listar_itens(array)
        elif opcao == 3:
            retirar_item(array)
        elif opcao == 4:
            retirar_todos_itens(array)
        elif opcao == 5:
            contar_maiores_que(array)
        elif opcao == 6:
            verificar_presenca(array)
        elif opcao == 7:
            encontrar_maior_menor(array)
        elif opcao == 8:
            print("Saindo...")
            break
        else:
            print("Opção inválida, tente novamente.")












